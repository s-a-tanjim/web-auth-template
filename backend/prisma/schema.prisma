generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id              Int      @id @default(autoincrement())
  user_name       String
  email           String   @unique
  password        String
  refresh_token   String   @default("")
  created_at      DateTime @default(now())
  updated_at      DateTime @default(now())
  deleted_at            DateTime?
}